{
  "include": ["**/*", "**/.*", "metro.config.js", "babel.config.js"],
  "extends": "@react-native/typescript-config/tsconfig.json",
  
  "compilerOptions": {
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@components": [
        "./src/components"
      ],
      "@constants": [
        "./src/constants"
      ],
      "@hooks": [
        "./src/hooks"
      ],
      "@navigator":[
        "./src/navigation"
      ],
      "@screens": [
        "./src/screens"
      ],
      "@services": [
        "./src/services"
      ],
      "@store": [
        "./src/store"
      ],
      "@utilities": [
        "./src/utilities"
      ],
      "@theme": [
        "./src/hooks/themecontext"
      ],
    },
  },
}
